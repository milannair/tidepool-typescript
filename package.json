{
  "name": "tidepool-client",
  "version": "0.1.0",
  "description": "TypeScript client library for Tidepool vector database services.",
  "license": "MIT",
  "author": "Milan Nair",
  "repository": {
    "type": "git",
    "url": "https://github.com/milannair/tidepool-clients.git",
    "directory": "tidepool-typescript"
  },
  "homepage": "https://github.com/milannair/tidepool-clients/tree/main/tidepool-typescript",
  "bugs": {
    "url": "https://github.com/milannair/tidepool-clients/issues"
  },
  "keywords": ["vector", "database", "search", "embeddings", "tidepool", "similarity"],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "docs"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "npm run build && node --test tests/*.test.mjs"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "typescript": "^5.4.0"
  }
}
